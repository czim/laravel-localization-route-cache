{
    "name": "xaoc/laravel-localization-route-cache",
    "description": "Translated route caching solution for laravel localization",
    "keywords": [
        "localization",
        "translations",
        "route",
        "cache"
    ],
    "homepage": "https://github.com/czim/laravel-localization-route-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com",
            "homepage": "http://www.pixelindustries.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "mcamara/laravel-localization": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\LaravelLocalizationRouteCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\LaravelLocalizationRouteCache\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
